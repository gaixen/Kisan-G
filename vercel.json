{
"version": 2,
"builds": [
    {
    "src": "package.json",
    "use": "@vercel/static-build",
    "config": {
        "distDir": "build"
    }
    },
    {
    "src": "server/app.py",
    "use": "@vercel/python"
    }
],
"rewrites": [
    {
    "source": "/api/(.*)",
    "destination": "/server/app.py"
    },
    {
    "source": "/(.*)",
    "destination": "/index.html"
    }
]
}